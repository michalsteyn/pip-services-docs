  &lt;span style=&#34;color:#d88200&#34;&gt;&amp;#39;param2&amp;#39;&lt;/span&gt;&lt;span style=&#34;color:#111&#34;&gt;,&lt;/span&gt; &lt;span style=&#34;color:#ae81ff&#34;&gt;345&lt;/span&gt;
