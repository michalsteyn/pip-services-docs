      <description>
        
        
        &lt;pre tabindex=&#34;0&#34;&gt;&lt;code&gt;# In-memory credential store
descriptor: &amp;quot;pip-services:credential-store:memory:default:1.0&amp;quot;
mongo:
  username: admin
  password: pass123

# MongoDb persistence component
descriptor: &amp;quot;myservice:mypersistance:mongodb:default:1.0&amp;quot;
connection:
  ...
